import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();

        for (int i = 0; i < n; i++) {
            char start = (char) ('A' + i);
            for (int j = 0; j <= i; j++) {
                System.out.print((char)(start + j) + " ");
            }
            System.out.println();
        }
    }
}
