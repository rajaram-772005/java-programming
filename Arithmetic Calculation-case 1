import java.io.*;
import java.util.*;

public class Solution 
{
    public static void main(String[] args)
    {
        Scanner obj = new Scanner(System.in);

        int num1 = obj.nextInt();
        int num2 = obj.nextInt();
        char operator = obj.next().charAt(0); 
        double result;
        
        switch(operator)
        {
            case '+':
                result = (double) num1 + num2;
                System.out.printf("Addition of two number is %.1f\n", result);
                break;
                
            case '-':
                result = (double) num1 - num2;
                System.out.printf("Subtraction of two number is %.1f\n", result);
                break;
                
            case '*':
                result = (double) num1 * num2;
                System.out.printf("Multiplication of two number is %.1f\n", result);
                break;
                
            case '/':
                if (num2 != 0)
                { 
                    result = (double) num1 / num2;
                    System.out.printf("Division of two number is %.1f\n", result);
                }
                else 
                {
                    System.out.println("Error: Division by zero!");
                }
                break;
            case '%':
                if (num2 != 0) { 
                    result = (double) num1 % num2;
                    System.out.printf("Modulo of two number is %.1f\n", result);
                }
                else
                {
                    System.out.println("Error: Division by zero for modulo!");
                }
                break;
            default:
                System.out.println("Invalid Input");
        }
        obj.close();
    }
}
